    <!-- Site Icons -->
    <link rel="shortcut icon" href="~/Home/images/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/Home/images/apple-touch-icon.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Home/css/bootstrap.min.css">
    <!-- Site CSS -->
