    <link href="https://cdn.datatables.net/1.11.1/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/buttons/2.0.0/css/buttons.dataTables.min.css" rel="stylesheet" />


</head>

<body>
    <!-- Start Main Top -->
    <div class="main-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <div class="custom-select-box">
                        <select id="basic" class="selectpicker show-tick form-control" data-placeholder="$ USD">
                            
                            <option>$ USD</option>
                           
                        </select>
                    </div>
                   
                    <div class="our-link">
                        <ul>
                            <li><a asp-action="ChefProfile" asp-controller="Home"><i class="fa fa-user s_color"></i> My Account</a></li>

                            <li><a asp-action="contactusChef" asp-controller="Home"><i class="fas fa-headset"></i> Contact Us</a></li>
                            <li><a  asp-action="ChefIndex" asp-controller="Home"><i class="fa fa-home fa-fw" aria-hidden="true"></i>&nbsp; Home</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <div class="login-box">
                        <div class="login-box">

                            <a asp-action="logout" asp-controller="Home" class="btn hvr-hover" style="color:white">LOG-OUT</a>
                        </div>


                    </div>
                   
                </div>
            </div>
        </div>
    </div>
    <!-- End Main Top -->
    <!-- Start Main Top -->
   
    <!-- End Main Top -->
        @RenderBody()
    <!-- Start Footer  -->
    <footer>
        <div class="footer-main">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-12 col-sm-12">
                        <div class="footer-link">
                            <h4>Information</h4>
                            <ul>
                                <li><a asp-action="aboutuschef" asp-controller="Home">About Us</a></li>


                                <li><a asp-action="termsGust" asp-controller="Home">Terms &amp; Conditions</a></li>
                                <li><a asp-action="privacypolicyGust" asp-controller="Home">Privacy Policy</a></li>

                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12 col-sm-12">
                        <div class="footer-top-box">
                            <h3>Subscribe to our email service</h3>
                            <form class="newsletter-box" method="post" asp-action="chefgenerate" asp-controller="QRCode">
                                <div class="form-group">
                                    <input class="form-control" type="text" name="Email" placeholder="Email Address*" />
                                    <i class="fa fa-envelope"></i>
                                </div>
                                <button class="btn hvr-hover" type="submit">Subscribe</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12 col-sm-12">
                        <div class="footer-top-box">
                            <h3>Social Media Accounts</h3>
                            <p></p>
                            <ul>
                                <li><a href="https://www.facebook.com/profile.php?id=61559611088226&mibextid=AEUHqQ"><i class="fab fa-facebook" aria-hidden="true"></i></a></li>
                                <li><a href="https://twitter.com/atheeradel07"><i class="fab fa-twitter" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.instagram.com/kvp_911?igsh=MTR6N2Vpa3I3MW0ycg%3D%3D&utm_source=qr"><i class="fab fa-instagram" aria-hidden="true"></i></a></li>


                                <li><a href="https://www.pinterest.com/ronnaca/masterchef/"><i class="fab fa-pinterest-p" aria-hidden="true"></i></a></li>

                            </ul>
                        </div>
                    </div>
                </div>
                <hr>
                
            </div>
        </div>
    </footer>
    <!-- End Footer  -->
    <!-- Start copyright  -->
    <div class="footer-copyright">
        <p class="footer-company">
            All Rights Reserved. &copy; 2024 <a href="#">MasterChef</a> Design By :
            <a href="https://html.design/">Atheer Adel</a>
        </p>
    </div>
    <!-- End copyright  -->

    <a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>

    <!-- ALL JS FILES -->
    <script src="~/Home/js/jquery-3.2.1.min.js"></script>
    <script src="~/Home/js/popper.min.js"></script>
    <script src="~/Home/js/bootstrap.min.js"></script>
    <!-- ALL PLUGINS -->
    <script src="~/Home/js/jquery.superslides.min.js"></script>
    <script src="~/Home/js/bootstrap-select.js"></script>
    <script src="~/Home/js/inewsticker.js"></script>
    <script src="~/Home/js/bootsnav.js."></script>
    <script src="~/Home/js/images-loded.min.js"></script>
